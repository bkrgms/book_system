{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      books: [{\n        id: 1,\n        title: \"Book One\",\n        author: \"Author A\",\n        pages: 200\n      }, {\n        id: 2,\n        title: \"Book Two\",\n        author: \"Author B\",\n        pages: 150\n      }, {\n        id: 3,\n        title: \"Book Three\",\n        author: \"Author C\",\n        pages: 300\n      }],\n      showAddBookModal: false,\n      newBook: {\n        title: \"\",\n        author: \"\",\n        pages: null\n      }\n    };\n  },\n  methods: {\n    logout() {\n      alert(\"Çıkış yapılıyor!\");\n      this.$router.push(\"/\"); // Ana giriş sayfasına yönlendirme\n    },\n    openAddBookModal() {\n      this.showAddBookModal = true;\n    },\n    closeAddBookModal() {\n      this.showAddBookModal = false;\n      this.newBook = {\n        title: \"\",\n        author: \"\",\n        pages: null\n      };\n    },\n    addBook() {\n      const newId = this.books.length + 1;\n      this.books.push({\n        id: newId,\n        ...this.newBook\n      });\n      this.closeAddBookModal();\n    },\n    editBook(book) {\n      alert(`Düzenleme işlemi: ${book.title}`);\n      // Düzenleme işlemi için modal açılabilir.\n    },\n    deleteBook(id) {\n      this.books = this.books.filter(book => book.id !== id);\n      alert(\"Kitap başarıyla silindi!\");\n    }\n  }\n};","map":{"version":3,"names":["data","books","id","title","author","pages","showAddBookModal","newBook","methods","logout","alert","$router","push","openAddBookModal","closeAddBookModal","addBook","newId","length","editBook","book","deleteBook","filter"],"sources":["C:\\Users\\bekir\\Desktop\\SoftwareEngineering\\project-apis\\kitap-sistemi\\src\\views\\AdminPanel.vue"],"sourcesContent":["<template>\r\n    <div class=\"admin-panel\">\r\n      <!-- Navbar -->\r\n      <nav class=\"navbar\">\r\n        <span class=\"project-name\">Admin Panel</span>\r\n        <button class=\"logout-button\" @click=\"logout\">Çıkış Yap</button>\r\n      </nav>\r\n  \r\n      <!-- Yönetici İşlemleri -->\r\n      <div class=\"admin-actions\">\r\n        <button @click=\"openAddBookModal\" class=\"action-button\">Kitap Ekle</button>\r\n      </div>\r\n  \r\n      <!-- Kitap Listesi -->\r\n      <div class=\"book-list\">\r\n        <h2>Mevcut Kitaplar</h2>\r\n        <table class=\"book-table\">\r\n          <thead>\r\n            <tr>\r\n              <th>ID</th>\r\n              <th>Kitap Adı</th>\r\n              <th>Yazar</th>\r\n              <th>İşlemler</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"book in books\" :key=\"book.id\">\r\n              <td>{{ book.id }}</td>\r\n              <td>{{ book.title }}</td>\r\n              <td>{{ book.author }}</td>\r\n              <td>\r\n                <button @click=\"editBook(book)\" class=\"edit-button\">Düzenle</button>\r\n                <button @click=\"deleteBook(book.id)\" class=\"delete-button\">Sil</button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n  \r\n      <!-- Kitap Ekle Modal -->\r\n      <div v-if=\"showAddBookModal\" class=\"modal\">\r\n        <div class=\"modal-content\">\r\n          <h2>Kitap Ekle</h2>\r\n          <form @submit.prevent=\"addBook\">\r\n            <div class=\"form-group\">\r\n              <label for=\"title\">Kitap Adı</label>\r\n              <input v-model=\"newBook.title\" id=\"title\" type=\"text\" required />\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label for=\"author\">Yazar</label>\r\n              <input v-model=\"newBook.author\" id=\"author\" type=\"text\" required />\r\n            </div>\r\n            <div class=\"form-group\">\r\n              <label for=\"pages\">Sayfa Sayısı</label>\r\n              <input v-model=\"newBook.pages\" id=\"pages\" type=\"number\" required />\r\n            </div>\r\n            <button type=\"submit\" class=\"save-button\">Kaydet</button>\r\n            <button type=\"button\" @click=\"closeAddBookModal\" class=\"cancel-button\">İptal</button>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        books: [\r\n          { id: 1, title: \"Book One\", author: \"Author A\", pages: 200 },\r\n          { id: 2, title: \"Book Two\", author: \"Author B\", pages: 150 },\r\n          { id: 3, title: \"Book Three\", author: \"Author C\", pages: 300 },\r\n        ],\r\n        showAddBookModal: false,\r\n        newBook: {\r\n          title: \"\",\r\n          author: \"\",\r\n          pages: null,\r\n        },\r\n      };\r\n    },\r\n    methods: {\r\n      logout() {\r\n        alert(\"Çıkış yapılıyor!\");\r\n        this.$router.push(\"/\"); // Ana giriş sayfasına yönlendirme\r\n      },\r\n      openAddBookModal() {\r\n        this.showAddBookModal = true;\r\n      },\r\n      closeAddBookModal() {\r\n        this.showAddBookModal = false;\r\n        this.newBook = { title: \"\", author: \"\", pages: null };\r\n      },\r\n      addBook() {\r\n        const newId = this.books.length + 1;\r\n        this.books.push({ id: newId, ...this.newBook });\r\n        this.closeAddBookModal();\r\n      },\r\n      editBook(book) {\r\n        alert(`Düzenleme işlemi: ${book.title}`);\r\n        // Düzenleme işlemi için modal açılabilir.\r\n      },\r\n      deleteBook(id) {\r\n        this.books = this.books.filter((book) => book.id !== id);\r\n        alert(\"Kitap başarıyla silindi!\");\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .admin-panel {\r\n    font-family: Arial, sans-serif;\r\n    padding: 20px;\r\n  }\r\n  \r\n  /* Navbar */\r\n  .navbar {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n    padding: 20px;\r\n    background-color: #f4f4f4;\r\n    border-bottom: 1px solid #ddd;\r\n  }\r\n  \r\n  .project-name {\r\n    font-size: 24px;\r\n    font-weight: bold;\r\n  }\r\n  \r\n  .logout-button {\r\n    padding: 10px 15px;\r\n    font-size: 16px;\r\n    background-color: #dc3545;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .logout-button:hover {\r\n    background-color: #c82333;\r\n  }\r\n  \r\n  /* Yönetici İşlemleri */\r\n  .admin-actions {\r\n    margin: 20px 0;\r\n  }\r\n  \r\n  .action-button {\r\n    padding: 10px 20px;\r\n    font-size: 18px;\r\n    background-color: #007bff;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .action-button:hover {\r\n    background-color: #0056b3;\r\n  }\r\n  \r\n  /* Kitap Tablosu */\r\n  .book-list {\r\n    margin-top: 20px;\r\n  }\r\n  \r\n  .book-table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  .book-table th,\r\n  .book-table td {\r\n    border: 1px solid #ddd;\r\n    padding: 10px;\r\n    text-align: left;\r\n  }\r\n  \r\n  .edit-button,\r\n  .delete-button {\r\n    padding: 5px 10px;\r\n    margin-right: 5px;\r\n    font-size: 14px;\r\n    border: none;\r\n    border-radius: 3px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .edit-button {\r\n    background-color: #ffc107;\r\n    color: white;\r\n  }\r\n  \r\n  .edit-button:hover {\r\n    background-color: #e0a800;\r\n  }\r\n  \r\n  .delete-button {\r\n    background-color: #dc3545;\r\n    color: white;\r\n  }\r\n  \r\n  .delete-button:hover {\r\n    background-color: #c82333;\r\n  }\r\n  \r\n  /* Modal */\r\n  .modal {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    background-color: rgba(0, 0, 0, 0.5);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n  \r\n  .modal-content {\r\n    background-color: white;\r\n    padding: 20px;\r\n    border-radius: 5px;\r\n    width: 400px;\r\n  }\r\n  \r\n  .form-group {\r\n    margin-bottom: 15px;\r\n  }\r\n  \r\n  .form-group label {\r\n    display: block;\r\n    margin-bottom: 5px;\r\n  }\r\n  \r\n  .form-group input {\r\n    width: 100%;\r\n    padding: 8px;\r\n    font-size: 14px;\r\n    border: 1px solid #ccc;\r\n    border-radius: 3px;\r\n  }\r\n  \r\n  .save-button {\r\n    padding: 10px 15px;\r\n    background-color: #28a745;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .save-button:hover {\r\n    background-color: #218838;\r\n  }\r\n  \r\n  .cancel-button {\r\n    padding: 10px 15px;\r\n    margin-left: 10px;\r\n    background-color: #6c757d;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 5px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .cancel-button:hover {\r\n    background-color: #5a6268;\r\n  }\r\n  </style>\r\n  "],"mappings":";;;AAiEE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,UAAU;QAAEC,MAAM,EAAE,UAAU;QAAEC,KAAK,EAAE;MAAI,CAAC,EAC5D;QAAEH,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,UAAU;QAAEC,MAAM,EAAE,UAAU;QAAEC,KAAK,EAAE;MAAI,CAAC,EAC5D;QAAEH,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,YAAY;QAAEC,MAAM,EAAE,UAAU;QAAEC,KAAK,EAAE;MAAI,CAAC,CAC/D;MACDC,gBAAgB,EAAE,KAAK;MACvBC,OAAO,EAAE;QACPJ,KAAK,EAAE,EAAE;QACTC,MAAM,EAAE,EAAE;QACVC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDG,OAAO,EAAE;IACPC,MAAMA,CAAA,EAAG;MACPC,KAAK,CAAC,kBAAkB,CAAC;MACzB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC,EAAE;IAC1B,CAAC;IACDC,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAACP,gBAAe,GAAI,IAAI;IAC9B,CAAC;IACDQ,iBAAiBA,CAAA,EAAG;MAClB,IAAI,CAACR,gBAAe,GAAI,KAAK;MAC7B,IAAI,CAACC,OAAM,GAAI;QAAEJ,KAAK,EAAE,EAAE;QAAEC,MAAM,EAAE,EAAE;QAAEC,KAAK,EAAE;MAAK,CAAC;IACvD,CAAC;IACDU,OAAOA,CAAA,EAAG;MACR,MAAMC,KAAI,GAAI,IAAI,CAACf,KAAK,CAACgB,MAAK,GAAI,CAAC;MACnC,IAAI,CAAChB,KAAK,CAACW,IAAI,CAAC;QAAEV,