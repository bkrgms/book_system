{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.set.difference.v2.js\";\nimport \"core-js/modules/es.set.intersection.v2.js\";\nimport \"core-js/modules/es.set.is-disjoint-from.v2.js\";\nimport \"core-js/modules/es.set.is-subset-of.v2.js\";\nimport \"core-js/modules/es.set.is-superset-of.v2.js\";\nimport \"core-js/modules/es.set.symmetric-difference.v2.js\";\nimport \"core-js/modules/es.set.union.v2.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport \"core-js/modules/esnext.iterator.some.js\";\nimport axios from 'axios';\nimport { getCurrentInstance } from 'vue';\nexport default {\n  data() {\n    return {\n      books: JSON.parse(localStorage.getItem('adminBooks')) || [],\n      showEditModal: false,\n      editBook: {\n        id: \"\",\n        title: \"\",\n        author: \"\",\n        image: \"\"\n      },\n      showAddModal: false,\n      newBook: {\n        title: \"\",\n        author: \"\",\n        image: \"\"\n      }\n    };\n  },\n  setup() {\n    const app = getCurrentInstance();\n    const emitter = app.appContext.config.globalProperties.emitter;\n    return {\n      emitter\n    };\n  },\n  methods: {\n    saveToLocalStorage() {\n      localStorage.setItem('adminBooks', JSON.stringify(this.books));\n      this.emitter.emit('books-updated');\n    },\n    async fetchBooks() {\n      if (this.books.length === 0) {\n        try {\n          const response = await axios.get(\"https://www.googleapis.com/books/v1/volumes?q=classic+literature&maxResults=40\");\n          const rawBooks = response.data.items || [];\n\n          // Benzersiz kitapları filtrele\n          const uniqueBooks = [];\n          const usedImages = new Set();\n          let id = 1;\n          for (const item of rawBooks) {\n            if (uniqueBooks.length >= 20) break; // Maksimum 20 kitap\n\n            if (item.volumeInfo.imageLinks) {\n              const imageUrl = item.volumeInfo.imageLinks.thumbnail;\n              if (!usedImages.has(imageUrl)) {\n                usedImages.add(imageUrl);\n                uniqueBooks.push({\n                  id: id++,\n                  title: item.volumeInfo.title || \"Bilinmeyen Başlık\",\n                  author: item.volumeInfo.authors ? item.volumeInfo.authors[0] : \"Bilinmeyen Yazar\",\n                  image: imageUrl\n                });\n              }\n            }\n          }\n          this.books = uniqueBooks;\n          this.saveToLocalStorage();\n        } catch (error) {\n          console.error(\"Kitaplar alınırken bir hata oluştu:\", error);\n        }\n      }\n    },\n    openEditModal(book) {\n      this.editBook = {\n        ...book\n      };\n      this.showEditModal = true;\n    },\n    saveEdit() {\n      if (!this.isBookUnique(this.editBook, this.editBook.id)) {\n        alert(\"Bu kitap zaten mevcut veya aynı resim URL'sine sahip başka bir kitap var!\");\n        return;\n      }\n      const index = this.books.findIndex(book => book.id === this.editBook.id);\n      if (index !== -1) {\n        this.books[index] = {\n          ...this.editBook\n        };\n        this.saveToLocalStorage();\n        alert(\"Kitap başarıyla güncellendi!\");\n      }\n      this.closeEditModal();\n    },\n    deleteBook(id) {\n      const index = this.books.findIndex(book => book.id === id);\n      if (index !== -1) {\n        if (confirm(\"Bu kitabı silmek istediğinizden emin misiniz?\")) {\n          // Önce kitabı sil\n          this.books.splice(index, 1);\n          // Sonra ID'leri yeniden sırala\n          this.reorderBookIds();\n          this.saveToLocalStorage();\n          alert(\"Kitap başarıyla silindi!\");\n        }\n      }\n    },\n    closeEditModal() {\n      this.showEditModal = false;\n      this.editBook = {\n        id: \"\",\n        title: \"\",\n        author: \"\",\n        image: \"\"\n      };\n    },\n    logout() {\n      this.$router.push(\"/\");\n    },\n    openAddModal() {\n      this.showAddModal = true;\n    },\n    closeAddModal() {\n      this.showAddModal = false;\n      this.newBook = {\n        title: \"\",\n        author: \"\",\n        image: \"\"\n      };\n    },\n    saveNewBook() {\n      if (!this.isBookUnique(this.newBook)) {\n        alert(\"Bu kitap zaten mevcut veya aynı resim URL'sine sahip başka bir kitap var!\");\n        return;\n      }\n      const maxId = Math.max(...this.books.map(b => parseInt(b.id)), 0);\n      const newBook = {\n        id: maxId + 1,\n        title: this.newBook.title,\n        author: this.newBook.author,\n        image: this.newBook.image || \"https://via.placeholder.com/150x225\"\n      };\n      this.books.push(newBook);\n      this.saveToLocalStorage();\n      this.closeAddModal();\n    },\n    isBookUnique(newBook, excludeId = null) {\n      return !this.books.some(book => {\n        if (book.id === excludeId) return false;\n\n        // Aynı kapak fotosu kontrolü\n        if (book.image === newBook.image) return true;\n\n        // Başlık ve yazar kontrolü (büyük/küçük harf duyarsız)\n        const sameTitle = book.title.toLowerCase() === newBook.title.toLowerCase();\n        const sameAuthor = book.author.toLowerCase() === newBook.author.toLowerCase();\n        return sameTitle && sameAuthor;\n      });\n    },\n    reorderBookIds() {\n      this.books = this.books.sort((a, b) => a.id - b.id) // Önce mevcut ID'lere göre sırala\n      .map((book, index) => ({\n        ...book,\n        id: index + 1 // Yeni ID'leri ata\n      }));\n    }\n  },\n  mounted() {\n    this.fetchBooks();\n  }\n};","map":{"version":3,"names":["axios","getCurrentInstance","data","books","JSON","parse","localStorage","getItem","showEditModal","editBook","id","title","author","image","showAddModal","newBook","setup","app","emitter","appContext","config","globalProperties","methods","saveToLocalStorage","setItem","stringify","emit","fetchBooks","length","response","