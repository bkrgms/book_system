{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      books: [{\n        id: 1,\n        title: \"Book One\",\n        author: \"Author A\"\n      }, {\n        id: 2,\n        title: \"Book Two\",\n        author: \"Author B\"\n      }, {\n        id: 3,\n        title: \"Book Three\",\n        author: \"Author C\"\n      }],\n      showAddModal: false,\n      showEditModal: false,\n      newBook: {\n        title: \"\",\n        author: \"\"\n      },\n      editBook: {\n        id: \"\",\n        title: \"\",\n        author: \"\"\n      }\n    };\n  },\n  methods: {\n    logout() {\n      alert(\"Çıkış yapılıyor!\");\n      this.$router.push(\"/\");\n    },\n    openAddModal() {\n      this.showAddModal = true;\n    },\n    closeAddModal() {\n      this.showAddModal = false;\n      this.newBook = {\n        title: \"\",\n        author: \"\"\n      };\n    },\n    addBook() {\n      const newId = this.books.length + 1;\n      this.books.push({\n        id: newId,\n        ...this.newBook\n      });\n      alert(\"Kitap başarıyla eklendi!\");\n      this.closeAddModal();\n    },\n    openEditModal(book) {\n      this.editBook = {\n        ...book\n      };\n      this.showEditModal = true;\n    },\n    closeEditModal() {\n      this.showEditModal = false;\n      this.editBook = {\n        id: \"\",\n        title: \"\",\n        author: \"\"\n      };\n    },\n    saveEdit() {\n      const index = this.books.findIndex(b => b.id === this.editBook.id);\n      if (index !== -1) {\n        this.books.splice(index, 1, {\n          ...this.editBook\n        });\n        alert(\"Kitap başarıyla güncellendi!\");\n      }\n      this.closeEditModal();\n    },\n    deleteBook(id) {\n      this.books = this.books.filter(book => book.id !== id);\n      alert(\"Kitap başarıyla silindi!\");\n    }\n  }\n};","map":{"version":3,"names":["data","books","id","title","author","showAddModal","showEditModal","newBook","editBook","methods","logout","alert","$router","push","openAddModal","closeAddModal","addBook","newId","length","openEditModal","book","closeEditModal","saveEdit","index","findIndex","b","splice","deleteBook","filter"],"sources":["C:\\Users\\bekir\\Desktop\\SoftwareEngineering\\project-apis\\kitap-sistemi\\src\\views\\AdminPanel.vue"],"sourcesContent":["<template>\r\n  <div class=\"admin-panel\">\r\n    <!-- Navbar -->\r\n    <nav class=\"navbar\">\r\n      <span class=\"project-name\">Admin Panel</span>\r\n      <button class=\"logout-button\" @click=\"logout\">Çıkış Yap</button>\r\n    </nav>\r\n\r\n    <!-- Kitap Yönetimi -->\r\n    <div class=\"book-list\">\r\n      <h2>Kitap Yönetimi</h2>\r\n      <button @click=\"openAddModal\" class=\"add-button\">+ Kitap Ekle</button>\r\n      <table class=\"book-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>ID</th>\r\n            <th>Kitap Adı</th>\r\n            <th>Yazar</th>\r\n            <th>İşlemler</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"book in books\" :key=\"book.id\">\r\n            <td>{{ book.id }}</td>\r\n            <td>{{ book.title }}</td>\r\n            <td>{{ book.author }}</td>\r\n            <td>\r\n              <button @click=\"openEditModal(book)\" class=\"action-button edit-button\">\r\n                Düzenle\r\n              </button>\r\n              <button @click=\"deleteBook(book.id)\" class=\"action-button delete-button\">\r\n                Sil\r\n              </button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n\r\n    <!-- Kitap Ekle Modal -->\r\n    <div v-if=\"showAddModal\" class=\"modal\">\r\n      <div class=\"modal-content\">\r\n        <h2>Kitap Ekle</h2>\r\n        <form @submit.prevent=\"addBook\">\r\n          <div class=\"form-group\">\r\n            <label for=\"title\">Kitap Adı</label>\r\n            <input v-model=\"newBook.title\" id=\"title\" type=\"text\" required />\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"author\">Yazar</label>\r\n            <input v-model=\"newBook.author\" id=\"author\" type=\"text\" required />\r\n          </div>\r\n          <div class=\"modal-actions\">\r\n            <button type=\"submit\" class=\"save-button\">Ekle</button>\r\n            <button type=\"button\" @click=\"closeAddModal\" class=\"cancel-button\">İptal</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Düzenleme Modal -->\r\n    <div v-if=\"showEditModal\" class=\"modal\">\r\n      <div class=\"modal-content\">\r\n        <h2>Kitap Düzenle</h2>\r\n        <form @submit.prevent=\"saveEdit\">\r\n          <div class=\"form-group\">\r\n            <label for=\"id\">ID</label>\r\n            <input v-model=\"editBook.id\" id=\"id\" type=\"text\" readonly />\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"title\">Kitap Adı</label>\r\n            <input v-model=\"editBook.title\" id=\"title\" type=\"text\" required />\r\n          </div>\r\n          <div class=\"form-group\">\r\n            <label for=\"author\">Yazar</label>\r\n            <input v-model=\"editBook.author\" id=\"author\" type=\"text\" required />\r\n          </div>\r\n          <div class=\"modal-actions\">\r\n            <button type=\"submit\" class=\"save-button\">Kaydet</button>\r\n            <button type=\"button\" @click=\"closeEditModal\" class=\"cancel-button\">İptal</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      books: [\r\n        { id: 1, title: \"Book One\", author: \"Author A\" },\r\n        { id: 2, title: \"Book Two\", author: \"Author B\" },\r\n        { id: 3, title: \"Book Three\", author: \"Author C\" },\r\n      ],\r\n      showAddModal: false,\r\n      showEditModal: false,\r\n      newBook: { title: \"\", author: \"\" },\r\n      editBook: { id: \"\", title: \"\", author: \"\" },\r\n    };\r\n  },\r\n  methods: {\r\n    logout() {\r\n      alert(\"Çıkış yapılıyor!\");\r\n      this.$router.push(\"/\");\r\n    },\r\n    openAddModal() {\r\n      this.showAddModal = true;\r\n    },\r\n    closeAddModal() {\r\n      this.showAddModal = false;\r\n      this.newBook = { title: \"\", author: \"\" };\r\n    },\r\n    addBook() {\r\n      const newId = this.books.length + 1;\r\n      this.books.push({ id: newId, ...this.newBook });\r\n      alert(\"Kitap başarıyla eklendi!\");\r\n      this.closeAddModal();\r\n    },\r\n    openEditModal(book) {\r\n      this.editBook = { ...book };\r\n      this.showEditModal = true;\r\n    },\r\n    closeEditModal() {\r\n      this.showEditModal = false;\r\n      this.editBook = { id: \"\", title: \"\", author: \"\" };\r\n    },\r\n    saveEdit() {\r\n      const index = this.books.findIndex((b) => b.id === this.editBook.id);\r\n      if (index !== -1) {\r\n        this.books.splice(index, 1, { ...this.editBook });\r\n        alert(\"Kitap başarıyla güncellendi!\");\r\n      }\r\n      this.closeEditModal();\r\n    },\r\n    deleteBook(id) {\r\n      this.books = this.books.filter((book) => book.id !== id);\r\n      alert(\"Kitap başarıyla silindi!\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.admin-panel {\r\n  font-family: Arial, sans-serif;\r\n  padding: 20px;\r\n}\r\n\r\n.navbar {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 20px;\r\n  background-color: #f4f4f4;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n.project-name {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n}\r\n\r\n.logout-button {\r\n  padding: 10px 20px;\r\n  background-color: #dc3545;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n/* Kitap Tablosu */\r\n.book-list {\r\n  margin-top: 20px;\r\n}\r\n\r\n.add-button {\r\n  margin-bottom: 20px;\r\n  padding: 12px 20px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  font-size: 16px;\r\n  cursor: pointer;\r\n}\r\n\r\n.book-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\n.book-table th,\r\n.book-table td {\r\n  border: 1px solid #ddd;\r\n  padding: 12px;\r\n}\r\n\r\n.action-button {\r\n  padding: 10px 15px;\r\n  margin-right: 10px;\r\n  font-size: 14px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.edit-button {\r\n  background-color: #ffc107;\r\n  color: white;\r\n}\r\n\r\n.delete-button {\r\n  background-color: #dc3545;\r\n  color: white;\r\n}\r\n\r\n.action-button:hover {\r\n  transform: scale(1.05);\r\n  transition: 0.2s;\r\n}\r\n\r\n/* Modal */\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-content {\r\n  background-color: white;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  width: 400px;\r\n  text-align: center;\r\n}\r\n\r\n.save-button,\r\n.cancel-button {\r\n  padding: 10px 20px;\r\n  font-size: 14px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.save-button {\r\n  background-color: #28a745;\r\n  color: white;\r\n}\r\n\r\n.cancel-button {\r\n  background-color: #6c757d;\r\n  color: white;\r\n}\r\n</style>\r\n"],"mappings":";;;AAwFA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,UAAU;QAAEC,MAAM,EAAE;MAAW,CAAC,EAChD;QAAEF,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,UAAU;QAAEC,MAAM,EAAE;MAAW,CAAC,EAChD;QAAEF,EAAE,EAAE,CAAC;QAAEC,KAAK,EAAE,YAAY;QAAEC,MAAM,EAAE;MAAW,CAAC,CACnD;MACDC,YAAY,EAAE,KAAK;MACnBC,aAAa,EAAE,KAAK;MACpBC,OAAO,EAAE;QAAEJ,KAAK,EAAE,EAAE;QAAEC,MAAM,E