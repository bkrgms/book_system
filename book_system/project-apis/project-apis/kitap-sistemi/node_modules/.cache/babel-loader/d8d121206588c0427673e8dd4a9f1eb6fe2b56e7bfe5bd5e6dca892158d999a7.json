{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport \"core-js/modules/esnext.iterator.map.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      books: JSON.parse(localStorage.getItem('adminBooks')) || [],\n      showEditModal: false,\n      editBook: {\n        id: \"\",\n        title: \"\",\n        author: \"\",\n        image: \"\"\n      },\n      showAddModal: false,\n      newBook: {\n        title: \"\",\n        author: \"\",\n        image: \"\"\n      }\n    };\n  },\n  methods: {\n    saveToLocalStorage() {\n      localStorage.setItem('adminBooks', JSON.stringify(this.books));\n    },\n    async fetchBooks() {\n      if (this.books.length === 0) {\n        try {\n          const response = await axios.get(\"https://www.googleapis.com/books/v1/volumes?q=classic+literature+OR+world+famous+books&maxResults=40\");\n          const rawBooks = response.data.items || [];\n          this.books = rawBooks.filter(item => item.volumeInfo.imageLinks).slice(0, 34).map((item, index) => ({\n            id: index + 1,\n            title: item.volumeInfo.title || \"Bilinmeyen Başlık\",\n            author: item.volumeInfo.authors ? item.volumeInfo.authors[0] : \"Bilinmeyen Yazar\",\n            image: item.volumeInfo.imageLinks.thumbnail\n          }));\n          this.saveToLocalStorage();\n        } catch (error) {\n          console.error(\"Kitaplar alınırken bir hata oluştu:\", error);\n        }\n      }\n    },\n    openEditModal(book) {\n      this.editBook = {\n        ...book\n      };\n      this.showEditModal = true;\n    },\n    saveEdit() {\n      const index = this.books.findIndex(book => book.id === this.editBook.id);\n      if (index !== -1) {\n        this.books[index] = {\n          ...this.editBook\n        };\n        this.saveToLocalStorage();\n        alert(\"Kitap başarıyla güncellendi!\");\n      }\n      this.closeEditModal();\n    },\n    deleteBook(id) {\n      const index = this.books.findIndex(book => book.id === id);\n      if (index !== -1) {\n        if (confirm(\"Bu kitabı silmek istediğinizden emin misiniz?\")) {\n          this.books.splice(index, 1);\n          this.saveToLocalStorage();\n          alert(\"Kitap başarıyla silindi!\");\n        }\n      }\n    },\n    closeEditModal() {\n      this.showEditModal = false;\n      this.editBook = {\n        id: \"\",\n        title: \"\",\n        author: \"\",\n        image: \"\"\n      };\n    },\n    logout() {\n      this.$router.push(\"/\");\n    },\n    openAddModal() {\n      this.showAddModal = true;\n    },\n    closeAddModal() {\n      this.showAddModal = false;\n      this.newBook = {\n        title: \"\",\n        author: \"\",\n        image: \"\"\n      };\n    },\n    saveNewBook() {\n      const adminBooks = this.books.filter(book => book.id.toString().startsWith('admin_'));\n      const newId = adminBooks.length > 0 ? Math.max(...adminBooks.map(b => parseInt(b.id.replace('admin_', '')))) + 1 : 1;\n      const newBook = {\n        id: `admin_${newId}`,\n        title: this.newBook.title,\n        author: this.newBook.author,\n        image: this.newBook.image || \"https://via.placeholder.com/150x225\"\n      };\n      this.books.push(newBook);\n      this.saveToLocalStorage();\n      this.closeAddModal();\n    }\n  },\n  mounted() {\n    this.fetchBooks();\n  }\n};","map":{"version":3,"names":["axios","data","books","JSON","parse","localStorage","getItem","showEditModal","editBook","id","title","author","image","showAddModal","newBook","methods","saveToLocalStorage","setItem","stringify","fetchBooks","length","response","get","rawBooks","items","filter","item","volumeInfo","imageLinks","slice","map","index","authors","thumbnail","error","console","openEditModal","book","saveEdit","findIndex","alert","closeEditModal","deleteBook","confirm","splice","logout","$router","pu