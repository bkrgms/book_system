{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  data() {\n    return {\n      books: [{\n        id: 1,\n        title: \"Book One\",\n        author: \"Author A\",\n        description: \"\"\n      }, {\n        id: 2,\n        title: \"Book Two\",\n        author: \"Author B\",\n        description: \"\"\n      }, {\n        id: 3,\n        title: \"Book Three\",\n        author: \"Author C\",\n        description: \"\"\n      }],\n      showDescriptionModal: false,\n      showViewModal: false,\n      selectedBook: null\n    };\n  },\n  methods: {\n    logout() {\n      alert(\"Çıkış yapılıyor!\");\n      this.$router.push(\"/\");\n    },\n    editBook(book) {\n      alert(`Düzenleme işlemi: ${book.title}`);\n      // Düzenleme işlemi için gerekli kodlar buraya eklenecek.\n    },\n    deleteBook(id) {\n      this.books = this.books.filter(book => book.id !== id);\n      alert(\"Kitap başarıyla silindi!\");\n    },\n    addDescription(book) {\n      this.selectedBook = book;\n      this.showDescriptionModal = true;\n    },\n    viewDescription(book) {\n      this.selectedBook = book;\n      this.showViewModal = true;\n    },\n    saveDescription() {\n      alert(\"Açıklama kaydedildi!\");\n      this.closeDescriptionModal();\n    },\n    closeDescriptionModal() {\n      this.showDescriptionModal = false;\n      this.selectedBook = null;\n    },\n    closeViewModal() {\n      this.showViewModal = false;\n      this.selectedBook = null;\n    }\n  }\n};","map":{"version":3,"names":["data","books","id","title","author","description","showDescriptionModal","showViewModal","selectedBook","methods","logout","alert","$router","push","editBook","book","deleteBook","filter","addDescription","viewDescription","saveDescription","closeDescriptionModal","closeViewModal"],"sources":["C:\\Users\\bekir\\Desktop\\SoftwareEngineering\\project-apis\\kitap-sistemi\\src\\views\\AdminPanel.vue"],"sourcesContent":["<template>\r\n  <div class=\"admin-panel\">\r\n    <!-- Navbar -->\r\n    <nav class=\"navbar\">\r\n      <span class=\"project-name\">Admin Panel</span>\r\n      <button class=\"logout-button\" @click=\"logout\">Çıkış Yap</button>\r\n    </nav>\r\n\r\n    <!-- Kitap Tablosu -->\r\n    <div class=\"book-list\">\r\n      <h2>Kitap Yönetimi</h2>\r\n      <table class=\"book-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>ID</th>\r\n            <th>Kitap Adı</th>\r\n            <th>Yazar</th>\r\n            <th>İşlemler</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"book in books\" :key=\"book.id\">\r\n            <td>{{ book.id }}</td>\r\n            <td>{{ book.title }}</td>\r\n            <td>{{ book.author }}</td>\r\n            <td>\r\n              <button @click=\"editBook(book)\" class=\"edit-button\">Düzenle</button>\r\n              <button @click=\"deleteBook(book.id)\" class=\"delete-button\">Sil</button>\r\n              <button @click=\"addDescription(book)\" class=\"description-button\">Açıklama Ekle</button>\r\n              <button @click=\"viewDescription(book)\" class=\"view-button\">Açıklamayı Gör</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n\r\n    <!-- Açıklama Ekle Modal -->\r\n    <div v-if=\"showDescriptionModal\" class=\"modal\">\r\n      <div class=\"modal-content\">\r\n        <h2>{{ selectedBook.title }} - Açıklama Ekle</h2>\r\n        <textarea v-model=\"selectedBook.description\" rows=\"5\" placeholder=\"Kitap açıklaması girin...\"></textarea>\r\n        <div class=\"modal-actions\">\r\n          <button @click=\"saveDescription\" class=\"save-button\">Kaydet</button>\r\n          <button @click=\"closeDescriptionModal\" class=\"cancel-button\">Kapat</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Açıklama Gör Modal -->\r\n    <div v-if=\"showViewModal\" class=\"modal\">\r\n      <div class=\"modal-content\">\r\n        <h2>{{ selectedBook.title }} - Açıklama</h2>\r\n        <p>{{ selectedBook.description || 'Bu kitap için açıklama yok.' }}</p>\r\n        <button @click=\"closeViewModal\" class=\"cancel-button\">Kapat</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      books: [\r\n        { id: 1, title: \"Book One\", author: \"Author A\", description: \"\" },\r\n        { id: 2, title: \"Book Two\", author: \"Author B\", description: \"\" },\r\n        { id: 3, title: \"Book Three\", author: \"Author C\", description: \"\" },\r\n      ],\r\n      showDescriptionModal: false,\r\n      showViewModal: false,\r\n      selectedBook: null,\r\n    };\r\n  },\r\n  methods: {\r\n    logout() {\r\n      alert(\"Çıkış yapılıyor!\");\r\n      this.$router.push(\"/\");\r\n    },\r\n    editBook(book) {\r\n      alert(`Düzenleme işlemi: ${book.title}`);\r\n      // Düzenleme işlemi için gerekli kodlar buraya eklenecek.\r\n    },\r\n    deleteBook(id) {\r\n      this.books = this.books.filter((book) => book.id !== id);\r\n      alert(\"Kitap başarıyla silindi!\");\r\n    },\r\n    addDescription(book) {\r\n      this.selectedBook = book;\r\n      this.showDescriptionModal = true;\r\n    },\r\n    viewDescription(book) {\r\n      this.selectedBook = book;\r\n      this.showViewModal = true;\r\n    },\r\n    saveDescription() {\r\n      alert(\"Açıklama kaydedildi!\");\r\n      this.closeDescriptionModal();\r\n    },\r\n    closeDescriptionModal() {\r\n      this.showDescriptionModal = false;\r\n      this.selectedBook = null;\r\n    },\r\n    closeViewModal() {\r\n      this.showViewModal = false;\r\n      this.selectedBook = null;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.admin-panel {\r\n  font-family: Arial, sans-serif;\r\n  padding: 20px;\r\n}\r\n\r\n/* Navbar */\r\n.navbar {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 20px;\r\n  background-color: #f4f4f4;\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\n.project-name {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n}\r\n\r\n.logout-button {\r\n  padding: 10px 15px;\r\n  font-size: 16px;\r\n  background-color: #dc3545;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n/* Kitap Tablosu */\r\n.book-list {\r\n  margin-top: 20px;\r\n}\r\n\r\n.book-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\n.book-table th,\r\n.book-table td {\r\n  border: 1px solid #ddd;\r\n  padding: 10px;\r\n  text-align: left;\r\n}\r\n\r\n.edit-button,\r\n.delete-button,\r\n.description-button,\r\n.view-button {\r\n  padding: 5px 10px;\r\n  margin-right: 5px;\r\n  font-size: 14px;\r\n  border: none;\r\n  border-radius: 3px;\r\n  cursor: pointer;\r\n}\r\n\r\n.edit-button {\r\n  background-color: #ffc107;\r\n  color: white;\r\n}\r\n\r\n.delete-button {\r\n  background-color: #dc3545;\r\n  color: white;\r\n}\r\n\r\n.description-button {\r\n  background-color: #007bff;\r\n  color: white;\r\n}\r\n\r\n.view-button {\r\n  background-color: #28a745;\r\n  color: white;\r\n}\r\n\r\n/* Modal */\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-content {\r\n  background-color: white;\r\n  padding: 20px;\r\n  border-radius: 5px;\r\n  width: 400px;\r\n  text-align: center;\r\n}\r\n\r\ntextarea {\r\n  width: 100%;\r\n  padding: 10px;\r\n  font-size: 14px;\r\n  margin-top: 10px;\r\n}\r\n\r\n.modal-actions {\r\n  margin-top: 20px;\r\n}\r\n\r\n.save-button {\r\n  padding: 10px 15px;\r\n  background-color: #28a745;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\n.cancel-button {\r\n  padding: 10px 15p