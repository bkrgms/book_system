eader h1 {\r\n  font-size: 36px;\r\n  margin: 0;\r\n}\r\n\r\n/* Seçim Alanı */\r\n.selection-container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 100%;\r\n  flex-grow: 1;\r\n}\r\n\r\n.selection-card {\r\n  background: white;\r\n  width: 400px;\r\n  padding: 30px;\r\n  border-radius: 10px;\r\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);\r\n  text-align: center;\r\n}\r\n\r\n.selection-card h2 {\r\n  font-size: 18px;\r\n  color: #6c757d;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n/* Butonlar */\r\n.buttons {\r\n  display: flex;\r\n  justify-content: space-around;\r\n}\r\n\r\n.button {\r\n  padding: 12px 20px;\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease, transform 0.2s ease;\r\n}\r\n\r\n.user-button {\r\n  background-color: #28a745;\r\n  color: white;\r\n}\r\n\r\n.user-button:hover {\r\n  background-color: #218838;\r\n  transform: scale(1.05);\r\n}\r\n\r\n.admin-button {\r\n  background-color: #dc3545;\r\n  color: white;\r\n}\r\n\r\n.admin-button:hover {\r\n  background-color: #a71d2a;\r\n  transform: scale(1.05);\r\n}\r\n\r\n/* Footer */\r\n.footer {\r\n  text-align: center;\r\n  padding: 10px 0;\r\n  background-color: #f8f9fa;\r\n  color: #6c757d;\r\n  font-size: 14px;\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"mappings":";AA0BA,eAAe;EACbA,OAAO,EAAE;IACPC,YAAYA,CAAA,EAAG;MACb,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,OAAO,CAAC;IAC5B,CAAC;IACDC,kBAAkBA,CAAA,EAAG;MACnB,IAAI,CAACF,OAAO,CAACC,IAAI,CAAC,cAAc,CAAC;IACnC;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}