{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nimport axios from \"axios\";\nimport BookList from \"./components/BooksList.vue\";\nexport default {\n  components: {\n    BookList\n  },\n  data() {\n    return {\n      searchQuery: \"\",\n      // Arama sorgusu\n      books: [],\n      // Google API'den gelen tüm kitaplar\n      filteredBooks: [] // Arama sonrası filtrelenmiş kitaplar\n    };\n  },\n  methods: {\n    fetchBooks() {\n      // Kitapları API'den çekme işlemi\n      axios.get(\"https://www.googleapis.com/books/v1/volumes?q=harry+potter\").then(response => {\n        this.books = response.data.items; // Kitapları kaydediyoruz\n        this.filteredBooks = this.books; // Başlangıçta tüm kitapları gösteriyoruz\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    searchBooks() {\n      if (this.searchQuery.trim()) {\n        // Arama kutusuna yazılmışsa, kitapları filtreleyin\n        this.filteredBooks = this.books.filter(book => book.volumeInfo.title.toLowerCase().includes(this.searchQuery.toLowerCase()));\n      } else {\n        // Arama kutusu boşsa, tüm kitapları tekrar göster\n        this.filteredBooks = this.books;\n      }\n    }\n  },\n  created() {\n    this.fetchBooks(); // Sayfa ilk açıldığında kitapları yükle\n  }\n};","map":{"version":3,"names":["axios","BookList","components","data","searchQuery","books","filteredBooks","methods","fetchBooks","get","then","response","items","catch","error","console","searchBooks","trim","filter","book","volumeInfo","title","toLowerCase","includes","created"],"sources":["C:\\Users\\bekir\\Desktop\\SoftwareEngineering\\project-apis\\kitap-sistemi\\src\\App.vue"],"sourcesContent":["<template>\n  <div>\n    <!-- Arama Input ve Butonu -->\n    <div>\n      <input v-model=\"searchQuery\" @input=\"searchBooks\" placeholder=\"Kitap ara...\" />\n      <button @click=\"searchBooks\">Arama</button>\n    </div>\n    \n    <!-- Kitap Listesi -->\n    <div v-if=\"filteredBooks.length > 0\">\n      <BookList :books=\"filteredBooks\" />\n    </div>\n    <div v-else>\n      <p>Kitap bulunamadı.</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport BookList from \"./components/BooksList.vue\";\n\nexport default {\n  components: {\n    BookList,\n  },\n  data() {\n    return {\n      searchQuery: \"\", // Arama sorgusu\n      books: [], // Google API'den gelen tüm kitaplar\n      filteredBooks: [], // Arama sonrası filtrelenmiş kitaplar\n    };\n  },\n  methods: {\n    fetchBooks() {\n      // Kitapları API'den çekme işlemi\n      axios\n        .get(\"https://www.googleapis.com/books/v1/volumes?q=harry+potter\")\n        .then((response) => {\n          this.books = response.data.items; // Kitapları kaydediyoruz\n          this.filteredBooks = this.books; // Başlangıçta tüm kitapları gösteriyoruz\n        })\n        .catch((error) => {\n          console.error(error);\n        });\n    },\n    searchBooks() {\n      if (this.searchQuery.trim()) {\n        // Arama kutusuna yazılmışsa, kitapları filtreleyin\n        this.filteredBooks = this.books.filter((book) =>\n          book.volumeInfo.title.toLowerCase().includes(this.searchQuery.toLowerCase())\n        );\n      } else {\n        // Arama kutusu boşsa, tüm kitapları tekrar göster\n        this.filteredBooks = this.books;\n      }\n    }\n  },\n  created() {\n    this.fetchBooks(); // Sayfa ilk açıldığında kitapları yükle\n  }\n};\n</script>\n"],"mappings":";;AAmBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,4BAA4B;AAEjD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MAAE;MACjBC,KAAK,EAAE,EAAE;MAAE;MACXC,aAAa,EAAE,EAAE,CAAE;IACrB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX;MACAR,KAAI,CACDS,GAAG,CAAC,4DAA4D,EAChEC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACN,KAAI,GAAIM,QAAQ,CAACR,IAAI,CAACS,KAAK,EAAE;QAClC,IAAI,CAACN,aAAY,GAAI,IAAI,CA