{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      books: [],\n      // SOAP API'den alınan kitaplar\n      showEditModal: false,\n      editBookData: {} // Düzenlenen kitap verileri\n    };\n  },\n  methods: {\n    async fetchBooks() {\n      try {\n        // SOAP API'den kitapları al\n        const response = await axios.post(\"http://localhost:8001/wsdl\", {\n          method: \"fetchBooks\"\n        });\n        this.books = response.data.books || [];\n      } catch (error) {\n        console.error(\"Kitaplar alınırken bir hata oluştu:\", error);\n      }\n    },\n    editBook(book) {\n      this.editBookData = {\n        ...book\n      }; // Düzenleme için seçilen kitabı modal'a yükle\n      this.showEditModal = true;\n    },\n    async saveEdit() {\n      try {\n        const response = await axios.post(\"http://localhost:8001/wsdl\", {\n          method: \"editBook\",\n          id: this.editBookData.id,\n          title: this.editBookData.title,\n          author: this.editBookData.author\n        });\n        if (response.data.success) {\n          alert(\"Kitap başarıyla güncellendi!\");\n          this.fetchBooks(); // Güncellenen kitapları yeniden al\n        } else {\n          alert(\"Güncelleme başarısız: \" + response.data.message);\n        }\n        this.closeEditModal();\n      } catch (error) {\n        console.error(\"Güncelleme hatası:\", error);\n      }\n    },\n    async deleteBook(id) {\n      try {\n        const response = await axios.post(\"http://localhost:8001/wsdl\", {\n          method: \"deleteBook\",\n          id: id\n        });\n        if (response.data.success) {\n          alert(\"Kitap başarıyla silindi!\");\n          this.fetchBooks(); // Silinen kitapları yeniden al\n        } else {\n          alert(\"Silme işlemi başarısız: \" + response.data.message);\n        }\n      } catch (error) {\n        console.error(\"Silme hatası:\", error);\n      }\n    },\n    closeEditModal() {\n      this.showEditModal = false;\n      this.editBookData = {};\n    }\n  },\n  mounted() {\n    this.fetchBooks(); // Sayfa yüklendiğinde kitapları al\n  }\n};","map":{"version":3,"names":["axios","data","books","showEditModal","editBookData","methods","fetchBooks","response","post","method","error","console","editBook","book","saveEdit","id","title","author","success","alert","message","closeEditModal","deleteBook","mounted"],"sources":["C:\\Users\\bekir\\Desktop\\SoftwareEngineering\\project-apis\\kitap-sistemi\\src\\views\\AdminPanel.vue"],"sourcesContent":["<template>\r\n  <div class=\"admin-panel\">\r\n    <h1>Admin Paneli</h1>\r\n    <table class=\"book-table\">\r\n      <thead>\r\n        <tr>\r\n          <th>ID</th>\r\n          <th>Kitap Adı</th>\r\n          <th>Yazar</th>\r\n          <th>İşlemler</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"book in books\" :key=\"book.id\">\r\n          <td>{{ book.id }}</td>\r\n          <td>{{ book.title }}</td>\r\n          <td>{{ book.author }}</td>\r\n          <td>\r\n            <button @click=\"editBook(book)\">Düzenle</button>\r\n            <button @click=\"deleteBook(book.id)\">Sil</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <!-- Düzenleme Modal -->\r\n    <div v-if=\"showEditModal\" class=\"modal\">\r\n      <div class=\"modal-content\">\r\n        <h2>Kitap Düzenle</h2>\r\n        <form @submit.prevent=\"saveEdit\">\r\n          <label for=\"title\">Kitap Adı</label>\r\n          <input v-model=\"editBookData.title\" id=\"title\" type=\"text\" required />\r\n          <label for=\"author\">Yazar</label>\r\n          <input v-model=\"editBookData.author\" id=\"author\" type=\"text\" required />\r\n          <button type=\"submit\">Kaydet</button>\r\n          <button type=\"button\" @click=\"closeEditModal\">İptal</button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      books: [], // SOAP API'den alınan kitaplar\r\n      showEditModal: false,\r\n      editBookData: {}, // Düzenlenen kitap verileri\r\n    };\r\n  },\r\n  methods: {\r\n    async fetchBooks() {\r\n      try {\r\n        // SOAP API'den kitapları al\r\n        const response = await axios.post(\"http://localhost:8001/wsdl\", {\r\n          method: \"fetchBooks\",\r\n        });\r\n        this.books = response.data.books || [];\r\n      } catch (error) {\r\n        console.error(\"Kitaplar alınırken bir hata oluştu:\", error);\r\n      }\r\n    },\r\n    editBook(book) {\r\n      this.editBookData = { ...book }; // Düzenleme için seçilen kitabı modal'a yükle\r\n      this.showEditModal = true;\r\n    },\r\n    async saveEdit() {\r\n      try {\r\n        const response = await axios.post(\"http://localhost:8001/wsdl\", {\r\n          method: \"editBook\",\r\n          id: this.editBookData.id,\r\n          title: this.editBookData.title,\r\n          author: this.editBookData.author,\r\n        });\r\n        if (response.data.success) {\r\n          alert(\"Kitap başarıyla güncellendi!\");\r\n          this.fetchBooks(); // Güncellenen kitapları yeniden al\r\n        } else {\r\n          alert(\"Güncelleme başarısız: \" + response.data.message);\r\n        }\r\n        this.closeEditModal();\r\n      } catch (error) {\r\n        console.error(\"Güncelleme hatası:\", error);\r\n      }\r\n    },\r\n    async deleteBook(id) {\r\n      try {\r\n        const response = await axios.post(\"http://localhost:8001/wsdl\", {\r\n          method: \"deleteBook\",\r\n          id: id,\r\n        });\r\n        if (response.data.success) {\r\n          alert(\"Kitap başarıyla silindi!\");\r\n          this.fetchBooks(); // Silinen kitapları yeniden al\r\n        } else {\r\n          alert(\"Silme işlemi başarısız: \" + response.data.message);\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Silme hatası:\", error);\r\n      }\r\n    },\r\n    closeEditModal() {\r\n      this.showEditModal = false;\r\n      this.editBookData = {};\r\n    },\r\n  },\r\n  mounted() {\r\n    this.fetchBooks(); // Sayfa yüklendiğinde kitapları al\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.admin-panel {\r\n  font-family: Arial, sans-serif;\r\n  padding: 20px;\r\n}\r\n\r\n.book-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n  margin-top: 20px;\r\n}\r\n\r\n.book-table th,\r\n.book-table td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n  text-align: left;\r\n}\r\n\r\nbutton {\r\n  padding: 5px 10px;\r\n  margin: 5px;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n}\r\n\r\nbutton:hover {\r\n  opacity: 0.8;\r\n}\r\n\r\n.modal {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.modal-content {\r\n  background-color: white;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  width: 300px;\r\n}\r\n\r\n.modal-content form {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.modal-content input {\r\n  margin-bottom: 15px;\r\n  padding: 10px;\r\n  font-size: 14px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 5px;\r\n}\r\n</style>\r\n"],"mappings":"AA2CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MAAE;MACXC,aAAa,EAAE,KAAK;MACpBC,YAAY,EAAE,CAAC,CAAC,CAAE;IACpB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF;QACA,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,IAAI,CAAC,4BAA4B,EAAE;UAC9DC,MAAM,EAAE;QACV,CAAC,CAAC;QACF,IAAI,CAACP,KAAI,GAAIK,QAAQ,CAACN,IAAI,CAACC,KAAI,IAAK,EAAE;MACxC,EAAE,OAAOQ,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,qCAAqC,EAAEA,KAAK,CAAC;MAC7D;IACF,CAAC;IACDE,QAAQA,CAACC,IAAI,EAAE;MACb,IAAI,CAACT,YAAW,GAAI;QAAE,GAAGS;MAAK,CAAC,EAAE;MACjC,IAAI,CAACV,aAAY,GAAI,IAAI;IAC3B,CAAC;IACD,